    kubernetes.io/cluster-service: "true"
    kubernetes.io/name: "Pushgateway"
spec:
  ports:
    - port: 9091
      protocol: TCP
      targetPort: pushgateway
  selector:
    k8s-app: pushgateway-monitoring
